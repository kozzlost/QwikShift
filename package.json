{
      "name": "qwikshift",
      "version": "1.0.0",
      "description": "Location-based event staffing platform - Find vetted staff in 24 hours",
      "main": "server.js",
      "scripts": {
              "dev": "NODE_ENV=development nodemon server.js",
              "start": "NODE_ENV=production node server.js",
              "test": "jest --coverage",
              "test:integration": "jest --testMatch='**/*.integration.test.js'",
              "test:security": "npm audit && jest --testMatch='**/*.security.test.js'",
              "migrate": "node ./db/migrate.js",
              "seed": "node ./db/seed.js",
              "lint": "eslint .",
              "format": "prettier --write ."
      },
      "keywords": ["event", "staffing", "jobs", "workers", "gig-economy"],
      "author": "kozzlost",
      "license": "MIT",
      "engines": {
              "node": ">=16.0.0",
              "npm": ">=8.0.0"
      },
      "dependencies": {
              "express": "^4.18.2",
              "stripe": "^11.10.0",
              "nodemailer": "^6.9.1",
              "twilio": "^3.85.0",
              "bcryptjs": "^2.4.3",
              "jsonwebtoken": "^9.0.0",
              "speakeasy": "^2.0.0",
              "qrcode": "^1.5.0",
              "cors": "^2.8.5",
              "helmet": "^7.0.0",
              "dotenv": "^16.0.3",
              "body-parser": "^1.20.2",
              "uuid": "^9.0.0"
      },
      "devDependencies": {
              "jest": "^29.5.0",
              "supertest": "^6.3.3",
              "nodemon": "^2.0.22",
              "eslint": "^8.40.0",
              "prettier": "^2.8.8"
      }
}
